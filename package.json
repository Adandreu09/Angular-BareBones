{
  "name": "angular2-barebones",
  "author": "Dan Wahlin",
  "version": "0.8.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/danwahlin/angular2-barebones"
  },
  "scripts": {
    "postinstall": "npm run typings install",
    "build": "tsc",
    "build:w": "tsc --watch",
    "bundle": "node bundle.js",
    "bundle:prod": "node bundle.js --prod",
    "live": "live-server --open=src --quiet",
    "start": "concurrently \"npm run build:w\" \"npm run live\" ",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.0",
    "@angular/compiler": "2.0.0-rc.0",
    "@angular/core": "2.0.0-rc.0",
    "@angular/platform-browser": "2.0.0-rc.0",
    "@angular/platform-browser-dynamic": "2.0.0-rc.0",
    "@angular/http": "2.0.0-rc.0",
    "@angular/router": "2.0.0-rc.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "live-server": "^1.0.0",
    "typescript": "^1.8.10",
    "typings":"^0.8.1",
    "systemjs-builder": "^0.15.15",
    "yargs": "^4.6.0"
  }
}
