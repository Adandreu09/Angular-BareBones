    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "live-server": "^1.0.0",
    "typescript": "^1.8.10",
    "typings":"^0.8.1"
  }
